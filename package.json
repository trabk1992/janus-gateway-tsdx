{
  "name": "janus-gateway-tsdx",
  "author": "Giorgos Smyrnaios <smyrgeorge@gmail.com>",
  "version": "0.3.3",
  "license": "MIT",
  "description": "Modern typescript client for janus-gateway.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smyrgeorge/janus-gateway-tsdx.git"
  },
  "keywords": [
    "janus",
    "janus-gateway",
    "webrtc",
    "video",
    "audio"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/janus-gateway-tsdx.esm.js",
  "size-limit": [
    {
      "path": "dist/janus-gateway-tsdx.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/janus-gateway-tsdx.esm.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/bluebird": "^3.5.36",
    "husky": "^7.0.1",
    "size-limit": "^11.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "eventemitter2": "^6.4.4"
  }
}
